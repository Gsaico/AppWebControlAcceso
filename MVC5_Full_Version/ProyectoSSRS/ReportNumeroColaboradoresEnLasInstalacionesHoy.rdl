  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSourceIDCHECKDB</DataSourceName>
        <CommandText>SELECT        dbo.Areas.NombreArea as [Nombre Área], dbo.Areas.CentroCostos as [Centro de Costos], count(dbo.RegistrosDiarios.COD_Colaborador) as [Nro Total De Personal]
FROM            dbo.Areas INNER JOIN
                         dbo.Colaboradores ON dbo.Areas.ID_Area = dbo.Colaboradores.ID_Area INNER JOIN
                         dbo.RegistrosDiarios ON dbo.Colaboradores.COD_Colaborador = dbo.RegistrosDiarios.COD_Colaborador
						  WHERE     DATEPART(day, dbo.RegistrosDiarios.Fecha) =  datepart(day,getdate())
						 group by  dbo.Areas.NombreArea, dbo.Areas.CentroCostos</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Nombre_Área">
          <DataField>Nombre Área</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Centro_de_Costos">
          <DataField>Centro de Costos</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Nro_Total_De_Personal">
          <DataField>Nro Total De Personal</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
